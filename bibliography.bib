@Article{Kugler2018a,
  Title                    = {Multiloop functional renormalization group for general models},
  Author                   = {Fabian B. Kugler and Jan von Delft},
  Journal                  = {Phys. Rev. B},
  Year                     = {2018},

  Month                    = {jan},
  Number                   = {3},
  Volume                   = {97},

  Abstract                 = {We present multiloop flow equations in the functional renormalization group (fRG) framework for the four-point vertex and self-energy, formulated for a general fermionic many-body problem. This generalizes the previously introduced vertex flow [F. B. Kugler and J. von Delft, Phys. Rev. Lett. 120, 057403 (2018)] and provides the necessary corrections to the self-energy flow in order to complete the derivative of all diagrams involved in the truncated fRG flow. Due to its iterative one-loop structure, the multiloop flow is well suited for numerical algorithms, enabling improvement of many fRG computations. We demonstrate its equivalence to a solution of the (first-order) parquet equations in conjunction with the Schwinger-Dyson equation for the self-energy.},
  Doi                      = {10.1103/physrevb.97.035162},
  File                     = {:Kugler2018a.pdf:PDF},
  Groups                   = {fRG},
  Publisher                = {American Physical Society ({APS})}
}

@Article{KuglerPRL2018b,
  Title                    = {Multiloop Functional Renormalization Group That Sums Up All Parquet Diagrams},
  Author                   = {Fabian B. Kugler and Jan von Delft},
  Journal                  = {Phys. Rev. Lett.},
  Year                     = {2018},

  Month                    = {jan},
  Number                   = {5},
  Volume                   = {120},

  Abstract                 = {We present a multiloop flow equation for the four-point vertex in the functional renormalization group (FRG) framework. The multiloop flow consists of successive one-loop calculations and sums up all parquet diagrams to arbitrary order. This provides substantial improvement of FRG computations for the four-point vertex and, consequently, the self-energy. Using the x-ray–edge singularity as an example, we show that solving the multiloop FRG flow is equivalent to solving the (first-order) parquet equations and illustrate this with numerical results.},
  Doi                      = {10.1103/physrevlett.120.057403},
  File                     = {:Kugler2018b.pdf:PDF},
  Groups                   = {fRG},
  Publisher                = {American Physical Society ({APS})}
}

